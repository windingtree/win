{
  "name": "@windingtree/stays-models",
  "version": "0.0.1",
  "description": "win-stays videre data models",
  "author": "mfw78 <mfw78@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "videre",
    "p2p",
    "waku",
    "windingtree",
    "markets",
    "stays"
  ],
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/cjs/src/index.d.ts",
  "scripts": {
    "typechain": "typechain --target ethers-v5 --glob './node_modules/@windingtree/videre-contracts/artifacts/**/*.json' --out-dir src/typechain",
    "protoc:source": "rm -rf ./dist/proto && mkdir -p ./dist/proto && cp -R ./src/proto/*.proto ./dist/proto/",
    "protoc:local": "protoc --ts_out ./src/proto --proto_path ./src/proto ./src/proto/*.proto",
    "protoc:libs": "protoc --ts_out ./src/proto --proto_path ./node_modules/@windingtree/videre-sdk/dist/proto ./node_modules/@windingtree/videre-sdk/dist/proto/*.proto",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublish": "yarn protoc:local && yarn protoc:libs && yarn protoc:source && yarn tsc",
    "test": "mocha --loader=ts-node/esm --extension ts"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.6.0",
    "ethers": "^5.6.6",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.5.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@windingtree/videre-contracts": "^0.1.6",
    "@windingtree/videre-sdk": "^0.4.1",
    "chai": "^4.3.6",
    "chai-ethers": "^0.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  }
}
